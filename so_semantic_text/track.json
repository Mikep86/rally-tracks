{% import "rally.helpers" as rally with context %}

{% set p_use_pipelines = (use_pipelines | default(false)) %}
{% set p_elser_model_id = (elser_model_id | default(".elser_model_2")) %}
{% set p_calculate_body_vector = (calculate_body_vector | default(false)) %}
{% set p_enable_search = (enable_search | default(false)) %}
{% set p_use_nested_text_expansion = (use_nested_text_expansion | default(false)) %}

{
  "version": 2,
  "description": "Indexing benchmark using up to questions and answers from StackOverflow",
  "indices": [
    {
      "name": "so-semantic-text",
      "body": "index.json"
    }
  ],
  "corpora": [
    {
      "name": "so-semantic-text",
      "base-url": "https://rally-tracks.elastic.co/so",
      "documents": [
        {
          "source-file": "posts.json.bz2",
          "target-index": "so-semantic-text",
          "document-count": 36062278,
          "compressed-bytes": 9600716233,
          "uncompressed-bytes": 35564808298
        }
      ]
    }
  ],
  "operations": [
    {{ rally.collect(parts="operations/*.json") }}
  ],
  "challenges": [
    {{ rally.collect(parts="challenges/*.json") }}
  ]
}
